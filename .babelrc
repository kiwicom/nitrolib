{
  "presets": [
    "@babel/react",
    "@babel/typescript",
    [
      "@babel/env",
      {
        "targets": {
          "browsers": "last 2 versions"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/transform-runtime",
    "@babel/transform-react-constant-elements",
    "@babel/proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    "id",
    [
      "styled-components",
      {
        "ssr": true,
        "transpileTemplateLiterals": true
      }
    ],
    "ramda"
  ],
  "env": {
    "modules": {
      "presets": [
        [
          "@babel/env",
          {
            "modules": false,
            "targets": {
              "browsers": "last 2 versions"
            }
          }
        ]
      ]
    }
  }
}
