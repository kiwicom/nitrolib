{
  "name": "@kiwicom/nitro",
  "version": "0.60.0",
  "description": "Nitro's components.",
  "scripts": {
    "start": "yarn storybook",
    "build": "run-p babel types keys docs",
    "babel": "babel src -d lib --ignore '**/__tests__/*'",
    "modules": "cross-env BABEL_ENV=modules babel src -d lib --ignore '**/__tests__/*' && renamer --find .js --replace .mjs 'lib/**'",
    "types": "copyfiles -u 1 src/**/*.js.flow lib",
    "keys": "node etc/collectKeys.js",
    "docs": "node etc/docs",
    "docs:check": "node etc/docs/check.js",
    "relay": "relay-compiler --src src/ --schema schema.graphql --extensions jsx js",
    "relay:schema": "graphql get-schema",
    "test": "jest",
    "lint": "eslint './**/*.{js,jsx,js.flow}'",
    "typecheck": "flow check",
    "fetch": "node ./bin/nitro.js fetch",
    "fixtures": "node ./bin/nitro.js keys 'src/**/*.{js,jsx}' && yarn fetch && node ./etc/fetchFixtures.js",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o dist-storybook",
    "prepare": "rimraf lib && run-p test lint typecheck && yarn build",
    "localsync": "rimraf lib && yarn build && yalc push"
  },
  "keywords": [
    "react",
    "components"
  ],
  "author": "kiwicom",
  "files": [
    "bin",
    "lib",
    "types",
    ".flowconfig",
    "tkeys.json",
    "README.md"
  ],
  "bin": {
    "nitro": "./bin/nitro.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "@kiwicom/orbit-design-tokens": "^0.2.5",
    "chalk": "^2.4.1",
    "date-fns": "^2.0.0-alpha.20",
    "fs-extra": "^7.0.0",
    "i18n-extract": "^0.6.3",
    "libphonenumber-js": "^1.6.4",
    "idx": "^2.4.0",
    "lodash": "^4.17.11",
    "mailcheck": "^1.1.1",
    "node-fetch": "^2.2.1",
    "react-transition-group": "^2.5.0",
    "validator": "^10.8.0"
  },
  "peerDependencies": {
    "@kiwicom/orbit-components": "^0.21.0",
    "ramda": "^0.25.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-icons": "^2.2.7",
    "react-relay": "^1.7.0",
    "relay-runtime": "^1.7.0",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@kiwicom/orbit-components": "^0.21.0",
    "@kiwicom/translations": "^1.62.0",
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-info": "^4.0.2",
    "@storybook/addon-knobs": "^4.0.2",
    "@storybook/addon-links": "^4.0.2",
    "@storybook/addons": "^4.0.2",
    "@storybook/react": "^4.0.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-id": "^1.0.1",
    "babel-plugin-idx": "^2.4.0",
    "babel-plugin-ramda": "^1.6.3",
    "babel-plugin-relay": "^1.7.0",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-preset-airbnb": "^3.0.1",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.85.0",
    "graphql": "^14.0.2",
    "graphql-cli": "^2.17.0",
    "gulp": "^4.0.0",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.2.2",
    "moment": "^2.22.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "raf": "^3.4.1",
    "ramda": "^0.25.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-icons": "^2.2.7",
    "react-relay": "^1.7.0",
    "relay-compiler": "^1.7.0",
    "relay-runtime": "^1.7.0",
    "renamer": "^1.1.0",
    "rimraf": "^2.6.2",
    "styled-components": "^4.1.1"
  }
}
